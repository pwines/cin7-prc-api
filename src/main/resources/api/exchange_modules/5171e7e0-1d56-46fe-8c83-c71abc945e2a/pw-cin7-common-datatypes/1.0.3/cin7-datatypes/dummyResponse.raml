#%RAML 1.0 DataType

type: object
additionalProperties: true
properties:
  success:
    type: boolean
    required: true
  apiName:
    type: string
    required: true
  version:
    type: string
    required: true
  correlationId:
    type: string
    required: true
  timestamp:
    type: string
    required: true
  data:
    type: object
    additionalProperties: true
    properties:
      message:
        type: string
        required: true
    required: true